    "version": "1.1.0",
    "script": "import { CohereClient } from \"cohere-ai\";\nexport default async function cohereTextSummarizer({\n    text,\n    apiKey,\n    length,\n    format,\n    temperature\n}: NodeInputs) : NodeOutput  {\n    const cohere = new CohereClient({\n      token: apiKey,\n    });\n    const response = await cohere.summarize({\n        text: text,\n        length: length,\n        format: format,\n        temperature: temperature\n    });\n    return response;\n}",
