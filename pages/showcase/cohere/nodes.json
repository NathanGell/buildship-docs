    "version": "1.1.0",
    "script": "import fetch from 'node-fetch';\nexport default async function cohereTextGenerator({\n    prompt,\n    max_tokens,\n    truncate,\n    return_likelihoods,\n    apiKey\n}: NodeInputs) : NodeOutput  {\n    const response = await fetch('https://api.cohere.ai/v1/generate', {\n        method: 'POST',\n        headers: {\n            'Accept': 'application/json',\n            'Content-Type': 'application/json',\n            'Authorization': `Bearer ${apiKey}`\n        },\n        body: JSON.stringify({\n            max_tokens,\n            truncate,\n            return_likelihoods,\n            prompt\n        })\n    });\n    const data = await response.json();\n    return data;\n}",
