    "version": "1.1.0",
    "script": "import { MeiliSearch } from 'meilisearch';\n\nexport default async function meilisearchFullTextSearch({\n    host,\n    apiKey,\n    indexName,\n    query,\n    filterCriteria = '',\n    facetsDistribution = []\n}: NodeInputs) : NodeOutput {\n    const client = new MeiliSearch({\n        host: host,\n        apiKey: apiKey\n    });\n    const index = client.index(indexName);\n    const searchParams = {\n        filter: filterCriteria,\n        facetsDistribution: facetsDistribution.length > 0 ? facetsDistribution : null\n    };\n    const searchResults = await index.search(query, searchParams);\n    return searchResults.hits;\n}\n",
    "usageCounter": 58,
