    "version": "1.1.0",
    "script": "import { MeiliSearch } from 'meilisearch';\n\nexport default async function meilisearchHybridSearch({\n    host,\n    apiKey,\n    indexName,\n    query,\n    limit,\n    filter,\n    facetsDistribution,\n    sort,\n    semanticRatio,\n    embedder\n}: NodeInputs) : NodeOutput  {\n    const client = new MeiliSearch({\n        host: host,\n        apiKey: apiKey\n    });\n    const index = client.index(indexName);\n    const searchParams = {\n        limit,\n        hybrid: {\n            semanticRatio,\n            embedder,\n        },\n        ...(filter && { filter }),\n        ...(facetsDistribution && { facetsDistribution }),\n        ...(sort && { sort })\n    };\n\n    const searchResults = await index.search(query, searchParams);\n    return searchResults.hits;\n}\n",
