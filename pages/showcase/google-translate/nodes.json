    "version": "1.1.0",
    "script": "import { v2 } from \"@google-cloud/translate\";\n\nexport default async function languageCodeValidator({\n    languageCode\n}: NodeInputs) : NodeOutput  {\n    const translate = new v2.Translate();\n\n    const [languages] = await translate.getLanguages();\n    return languages.some(language => language.code === languageCode);\n}\n\n",
