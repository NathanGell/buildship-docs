    "version": "1.1.0",
    "script": "import { getSubtitles, getVideoDetails } from 'youtube-caption-extractor';\nexport default async function youtubeCaptionExtractor({ videoUrl, lang }: NodeInputs) : NodeOutput  {\n    try {\n        const videoID = new URL(videoUrl).searchParams.get('v');\n        const subtitles = await getSubtitles({\n            videoID,\n            lang\n        });\n        const videoDetails = await getVideoDetails({\n            videoID,\n            lang\n        });\n        return {\n            subtitles,\n            videoDetails\n        };\n    } catch (error) {\n        console.error('Error fetching subtitles or video details:', error);\n    }\n}",
