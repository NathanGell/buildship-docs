    "script": "import fetch from 'node-fetch';\n\nexport default async function retrieveTranscripts({\n    apiKey\n}) {\n    const result = await fetch(\"https://api.assemblyai.com/v2/transcript\", {\n        method: \"GET\",\n        headers: {\n            Authorization: apiKey,\n        },\n    });\n\n    if (!result.ok) {\n        const error = await result.json();\n        throw new Error(error.message);\n    }\n\n    return await result.json();\n}",
