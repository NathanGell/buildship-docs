    "version": "1.1.0",
    "script": "export default function concatPropertyValues({\n    jsonObject,\n    key,\n    property\n}: NodeInputs) : NodeOutput  {\n    let result = '';\n    const arrayToAppend = jsonObject[key];\n\n    if (Array.isArray(arrayToAppend)) {\n        arrayToAppend.forEach(item => {\n            if (item.hasOwnProperty(property)) {\n                result += item[property] + ' ';\n            }\n        });\n    }\n\n    return result.trim();\n}\n",
