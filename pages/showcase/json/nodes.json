    "version": "1.1.0",
    "script": "import json2csv from 'json2csv';\nimport {\n    Storage\n} from '@google-cloud/storage';\nexport default async function storeCSVinGCPStorageBucket({\n    jsonArray,\n    folderPath,\n    fileName\n}: NodeInputs) : NodeOutput  {\n    const csv = json2csv.parse(jsonArray);\n    const storage = new Storage();\n    const bucketName = process.env.BUCKET;\n    const file = storage.bucket(bucketName).file(folderPath + '/' + fileName);\n    await file.save(csv);\n    await file.makePublic();\n    return `https://storage.googleapis.com/${bucketName}/${folderPath}/${fileName}`;\n}",
